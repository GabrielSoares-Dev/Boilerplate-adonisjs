{
  "name": "boilerplate-adonisjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test:coverage": "nyc node ace test",
    "prepare": "husky install",
    "commit": "cz"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.0.2",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/sinon": "^10.0.16",
    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "nock": "^13.3.3",
    "nyc": "^15.1.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "sinon": "^16.0.0",
    "typescript": "~5.2",
    "youch": "^3.3.1",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "axios": "^1.5.0",
    "commitizen": "^4.3.0",
    "date-fns": "^2.30.0",
    "form-data": "^4.0.0",
    "luxon": "^3.4.3",
    "pg": "^8.11.3",
    "phc-bcrypt": "^1.0.8",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
